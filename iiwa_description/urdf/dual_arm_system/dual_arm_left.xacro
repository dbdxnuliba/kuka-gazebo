<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
  <xacro:property name="joint_friction" value="0.00" />
  <xacro:property name="joint_damping" value="2" />
  <xacro:property name="PIhalf" value="1.570796" />
  <xacro:property name="PI" value="3.141592" />
  <xacro:property name="twoPI" value="6.0" />
  <xacro:macro name="dual_arm_left" params="robot_name"> 
 

  <link name="L1_L">
    <inertial>
      <origin xyz="0 0 0.012168" rpy="0 0 0" />
      <mass value="0.10323" />
      <inertia ixx="0" ixy="0.1" ixz="0" iyy="0" iyz="0" izz="0.1" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://iiwa_description/meshes/Dual_Arm_Manipulator/Arm/L1.STL" />
      </geometry>
      <material
        name="">
        <color rgba="1 1 1 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://iiwa_description/meshes/Dual_Arm_Manipulator/Arm/L1.STL" />
      </geometry>
    </collision>
 </link>
 <joint
    name="J1_L" type="revolute">
    <origin xyz="-0.0707 -0.0892 0.2086" rpy="-${PIhalf} 0.7330 ${PIhalf}" />
    <parent link="base_link" />
    <child link="L1_L" />
    <axis xyz="0 0 1" />
    <limit
      lower="0"
      upper="${twoPI}"
      effort="20000"
      velocity="13" />
    <dynamics damping="${joint_damping}" friction = "0"/>
  </joint>

  <link name="L2_L">
    <inertial>
      <origin xyz="0.0019663 -0.056961 -0.00024619" rpy="0 0 0" />
      <mass value="0.10019" />
      <inertia ixx="0.030334" ixy="0" ixz="0" iyy="0.01" iyz="0.0" izz="0.031932" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://iiwa_description/meshes/Dual_Arm_Manipulator/Arm/L2.STL" />
      </geometry>
      <material
        name="">
        <color rgba="0.52941 0.54902 0.54902 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://iiwa_description/meshes/Dual_Arm_Manipulator/Arm/L2.STL" />
      </geometry>
    </collision>
  </link>
  <joint name="J2_L" type="revolute">
    <origin xyz="0 0 0.0893" rpy="${PIhalf} -${PIhalf} 0" />
    <parent link="L1_L" />
    <child link="L2_L" />
    <axis xyz="0 0 1" />
    <limit
      lower="-${PIhalf}"
      upper="${PIhalf}"
      effort="20000"
      velocity="13" />
    <dynamics damping="${joint_damping}" friction = "0"/>
  </joint>

  <link name="L3_L">
    <inertial>
      <origin xyz="0 0.0 0.054906237567049" rpy="0 0 0" />
      <mass value="0.088571511506745" />
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.01" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://iiwa_description/meshes/Dual_Arm_Manipulator/Arm/L3.STL" />
      </geometry>
      <material name="">
        <color rgba="1 1 1 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://iiwa_description/meshes/Dual_Arm_Manipulator/Arm/L3.STL" />
      </geometry>
    </collision>
  </link>
  <joint name="J3_L" type="revolute">
    <origin xyz="0.000 -0.1706 0" rpy="0 ${PIhalf} -${PIhalf}" />
    <parent link="L2_L" />
    <child link="L3_L" />
    <axis xyz="0 0 1" />
    <limit
      lower="-${PI}"
      upper="${PI}"
      effort="20000"
      velocity="13" />
    <dynamics damping="${joint_damping}" friction = "0"/>
  </joint>

  <link name="L4_L">
    <inertial>
      <origin xyz="0 -0.056686585128774 -0.0" rpy="0 0 0" />
      <mass value="0.0805726314171642" />
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.0179335303394939" iyz="0" izz="0.0181823116496743" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://iiwa_description/meshes/Dual_Arm_Manipulator/Arm/L4.STL" />
      </geometry>
      <material name="">
        <color rgba="0.501960784313725 0.501960784313725 0.501960784313725 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://iiwa_description/meshes/Dual_Arm_Manipulator/Arm/L4.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="J4_L"
    type="revolute">
    <origin xyz="0 -0.0291 0.1055" rpy="0 ${PIhalf} ${PIhalf}" />
    <parent link="L3_L" />
    <child link="L4_L" />
    <axis xyz="0 0 1" />
    <limit
      lower="-${PI}"
      upper="${PI}"
      effort="20000"
      velocity="13" />
    <dynamics damping="${joint_damping}" friction = "0"/>
  </joint>


  </xacro:macro>
</robot>